/** objects/effects/effect **/


/** objects/effects/effect_type/effect_type **/



class EffectType {
	integer type;

	protected constructor(integer type) {
		this.type = type;		
	}
	
	static Map<integer, Class> typeMap = [
		EFFECT_DAMAGE: EffectDamage,
	];
	static EffectType? get(integer type) {
		debug(type + " " + typeMap)
		if (EffectType.typeMap[type] != null) { return (typeMap[type]!)(type) }
		debugW("Cannot get feature for " + type);
		return null;
	}
}
